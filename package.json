{
  "name": "petertait.com",
  "description": "Website for petertait.com built with Gatsbyjs and includes Netlify CMS.",
  "version": "2.0.0",
  "author": "Peter Tait",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.0.66",
    "gatsby-image": "^2.0.23",
